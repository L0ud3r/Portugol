funcao fatorial(min, max)
    inteiro: res;
    res <- min;
    para i de min+1 ate max faca
        res <- res * i;
    fimpara;
    res;
fimfuncao;

funcao fatorial(x)
    fatorial(1, x);
fimfuncao;

inicio

inteiro: i;
i <- 5;

i <- fatorial(i);
escreva(i);

logico: a; #fix nisto!
a <- 1; #fix nisto!


